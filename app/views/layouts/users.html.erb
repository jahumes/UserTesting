<!doctype html>
<html>
<head>
  <meta charset="utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title><%= content_for?(:title) ? yield(:title) : "Usertesting" %></title>
  <meta name="description" content=""/>
  <meta name="author" content=""/>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag :application %>
  <%= csrf_meta_tags %>
  <%= yield(:head) %>
</head>
<body>
<%= render :partial => 'header' %>
<!-- Sidebar begins -->
<%= render :partial => 'sidebar' %>
<!-- Sidebar ends -->
<!-- Content begins -->
<div id="content">
  <%= render :partial => 'content_top' %>
  <!-- Breadcrumbs line -->
  <div class="breadLine">
    <%= render :partial => 'breadcrumbs'%>

    <div class="breadLinks">
      <ul>
        <li><a href="#" title=""><i class="icos-list"></i><span>Orders</span> <strong>(+58)</strong></a></li>
        <li><a href="#" title=""><i class="icos-check"></i><span>Tasks</span> <strong>(+12)</strong></a></li>
        <li class="has">
          <a title="">
            <i class="icos-money3"></i>
            <span>Invoices</span>
            <span><img src="/assets/elements/control/hasddArrow.png" alt="" /></span>
          </a>
          <ul>
            <li><a href="#" title=""><span class="icos-add"></span>New invoice</a></li>
            <li><a href="#" title=""><span class="icos-archive"></span>History</a></li>
            <li><a href="#" title=""><span class="icos-printer"></span>Print invoices</a></li>
          </ul>
        </li>
      </ul>
      <div class="clear"></div>
    </div>
  </div>
  <div class="wrapper">
    <%= render 'layouts/messages' %>
    <%= yield %>
  </div>

</div>
</body>
</html>